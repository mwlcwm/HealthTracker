{
  "name": "electron-hello-world",
  "version": "1.0.0",
  "main": "src/main.js",
  "scripts": {
    "start": "npm-run-all --parallel start-electron start-react",
    "start-electron": "electron .",
    "start-react": "cd mytracker && npm start",
    "build": "cd mytracker && npm run build",
    "serve": "serve -s mytracker/build"
  },
  "devDependencies": {
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.15",
    "electron": "^13.6.9",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "npm-run-all": "^4.1.5",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^3.5.2"
  },
  "build": {
    "appId": "com.example.yourapp",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "better-sqlite3": "^10.0.0",
    "cors": "^2.8.5",
    "electron": "^12.0.0",
    "express": "^4.19.2",
    "papaparse": "^5.4.1"
  }
}
